    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "f = open('ex1data1.txt', 'r')\n",
    "X = []\n",
    "Y = []\n",
    "\n",
    "for line in f:\n",
    "    x,y = tuple(line.split(','))\n",
    "    X.append(float(x))\n",
    "    Y.append(float(y))\n",
    "\n",
    "plt.plot(X,Y, 'x', color = 'r')\n",
    "plt.show()"
   ]
  }
